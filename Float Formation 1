public class Solution {
    public static void main(String[] args) {
        float value = 67.98f; 
       
        System.out.printf("%.6f%n", value);

        
        System.out.printf("%.4f%n", value);

      
        System.out.printf("%.2f%n", value);

      
        System.out.printf("%d%n", Math.round(value));
    }
}
