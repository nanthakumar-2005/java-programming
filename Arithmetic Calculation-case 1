import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner scanner = new Scanner(System.in); 
        int num1 = scanner.nextInt();
        int num2 = scanner.nextInt();
        char operator = scanner.next().charAt(0);

        double result; 

        switch (operator) {
            case '+': 
                result = num1 + num2;
                System.out.printf("Addition of two number is %.1f\n", result); 
                break; 
            case '-': 
                result = num1 - num2; 
                System.out.printf("Subtraction of two number is %.1f\n", result);
                break; 
            case '*': 
                result = num1 * num2;
                System.out.printf("Multiplication of two number is %.1f\n", result); 
                break; 
            case '/': 
                if (num2 != 0) { 
                    result = (double) num1 / num2;
                    System.out.printf("Division of two number is %.1f\n", result); 
                } else { 
                    System.out.println("Error! Division by zero is not allowed."); 
                }
                break; 
            case '%': 
                result = num1 % num2;
                System.out.printf("Modulo of two number is %.1f\n", result); 
                break; 
            default:  
                break;
        }

        scanner.close();
    }
}
